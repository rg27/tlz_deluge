		//Update the License App Trigger to False
		app_map = Map();
		app_map.put("License_App_Trigger",false);
		app_res = zoho.crm.updateRecord("Applications1",application_id,app_map);
		//Continue change ownership
		requestStr = requestMap.get("requests");
		// 	info "requestStr" + requestStr;
		requestObj = requestStr.toMap();
		// 	info "requestObj" + requestObj;
		req_id = requestObj.get("request_id");
		new_email = email_id;
		//use new owner
		data = Map();
		data.put("email_id",email_id);
		// Change Ownership api call
		resp1 = invokeurl
		[
			url :"https://sign.zoho.com/api/v1/requests/" + req_id + "/changeownership"
			type :PUT
			parameters:data
			connection:"sign_change_ownership"
		];
	}
}
else
{
	sign_fields = zoho.sign.getFieldIds();
	for each  sign_field in sign_fields.getjson("field_types")
	{
		if(sign_field.getjson("field_type_name").containsIgnoreCase("Signature"))
		{
			sign_field_type_id = sign_field.getjson("field_type_id");
			sign_field_category = sign_field.getjson("field_category");
			sign_field_type_name = sign_field.getjson("field_type_name");
		}
	}
	application_details = zoho.crm.getRelatedRecords("New_License_Application1","Applications1",application_id);
	filesLists = list();
	new_format_filesLists = List();
	final_format_filesLists = List();
	company_member_list = List();
	iterator_counter = 0;
	license_application_forms = application_details.getjson("License_Application_Form");
	for each  license in license_application_forms
	{
		file_id = license.getJson("file_Id");
		license_app_form = invokeurl
		[
			url :"https://www.zohoapis.com/crm/v4/files?id=" + file_id
			type :GET
			connection:"crm_file_upload"
		];
		filesLists.add(license_app_form);
	}
	license_type = ifnull(application_details.getjson("License_Type"),"");
	for each  terms_and_condition in terms_and_conditions
	{
		if(terms_and_condition.get("file_Name").containsIgnoreCase("Trade License Zone") && terms_and_condition.get("file_Name").containsIgnoreCase("terms"))
		{
			file_id = terms_and_condition.get("file_Id");
			newfile = "{\"id\":1, \"file_name\": " + file_id + "}";
			new_format_filesLists.add(newfile);
		}
		if(terms_and_condition.get("file_Name").containsIgnoreCase("IFZA") && (terms_and_condition.get("file_Name").containsIgnoreCase("T&C") || terms_and_condition.get("file_Name").containsIgnoreCase("T-C")))
		{
			file_id = terms_and_condition.get("file_Id");
			newfile = "{\"id\":2, \"file_name\": " + file_id + "}";
			new_format_filesLists.add(newfile);
		}
		if(terms_and_condition.get("file_Name").containsIgnoreCase("IFZA-DXB Pricing"))
		{
			file_id = terms_and_condition.get("file_Id");
			newfile = "{\"id\":3, \"file_name\": " + file_id + "}";
			new_format_filesLists.add(newfile);
		}
		if(terms_and_condition.get("file_Name").containsIgnoreCase("IFZA") && terms_and_condition.get("file_Name").containsIgnoreCase("Visa Licenses"))
		{
			file_id = terms_and_condition.get("file_Id");
			newfile = "{\"id\":4, \"file_name\": " + file_id + "}";
			new_format_filesLists.add(newfile);
		}
		if(terms_and_condition.get("file_Name").containsIgnoreCase("IFZA") && terms_and_condition.get("file_Name").containsIgnoreCase("Property Price List"))
		{
			file_id = terms_and_condition.get("file_Id");
			newfile = "{\"id\":5, \"file_name\": " + file_id + "}";
			new_format_filesLists.add(newfile);
		}
	}
	for each  new_file in new_format_filesLists.sort(true)
	{
		final_format_filesLists.add(new_file.get("file_name"));
	}
	for each  final_format_list in final_format_filesLists
	{
		download_response = invokeurl
		[
			url :"https://www.zohoapis.com/crm/v4/files?id=" + final_format_list
			type :GET
			connection:"crm_file_upload"
		];
		filesLists.add(download_response);
	}
	license_app_details = zoho.crm.getRelatedRecords("Company_Members","Applications1",application_id);
	// info "License App Details: " + license_app_details;
	for each  license_app in license_app_details
	{
		iterator_counter = iterator_counter + 1;
		company_email = ifnull(license_app.getjson("Email"),"");
		info "Company Email: " + company_email;
		company_roles = ifnull(license_app.getjson("Roles_s"),"");
		company_fname = ifnull(license_app.getjson("First_Name")," ");
		company_lname = ifnull(license_app.getjson("Last_Name")," ");
		passport_files = ifnull(license_app.getjson("Passport_file")," ");
		if(license_app.getjson("Roles_s").containsIgnoreCase("General Manager/Representative"))
		{
			gm_email = ifnull(license_app.getjson("Email"),"");
			gm_fname = ifnull(license_app.getjson("First_Name"),"");
			gm_lname = ifnull(license_app.getjson("Last_Name"),"");
			gm_name = gm_fname + " " + gm_lname;
			info "GM Name: " + gm_name;
		}
		for each  passport_file in passport_files
		{
			file_id = passport_file.getJson("file_Id");
			company_member_info_map = Map();
			company_member_info_map.put("company_member_name",company_fname + " " + company_lname);
			download_response2 = invokeurl
			[
				url :"https://www.zohoapis.com/crm/v4/files?id=" + file_id
				type :GET
				parameters:company_member_info_map
				connection:"crm_file_upload"
			];
			filesLists.add(download_response2);
		}
		InsideParam = Map();
		InsideParam.put("Email",company_email);
		InsideParam.put("First_Name",company_fname);
		InsideParam.put("Last_Name",company_lname);
		InsideParam.put("Roles_s",company_roles);
		company_member_list.add(InsideParam);
	}
	dataMap = Map();
	dataMap.put("requests",{"request_name":"Applications Form -" + account_name});
	requestMap = Map();
	requestMap.put("data",dataMap);
	created_document_response = zoho.sign.createDocument(filesLists,requestMap,"sign_crm2");
	info "Created Document Response: " + created_document_response;
	req_id = created_document_response.getjson("requests").getjson("request_id");
	info "Req_id: " + req_id;
	documents = created_document_response.getjson("requests").getjson("document_ids");
	info "Documents: " + documents;
	filelist = List();
	response = invokeurl
	[
		url :"https://sign.zoho.com/api/v1/requests/" + req_id + "/pdf"
		type :GET
		connection:"sign_crm"
	];
	response.setparamname("file");
	upload_res = invokeurl
	[
		url :"https://www.zohoapis.com/crm/v5/files"
		type :POST
		files:response
		connection:"crm_files"
	];
	fileid = upload_res.getJSON("data").get(0).getJSON("details").getJSON("id");
	//Map the data of the newly updated filename
	fmp = Map();
	fmp.put("file_id",fileid);
	filelist.add(fmp);
	//Update the Passport 
	mp = Map();
	mp.put("Combined_License_Application_Status",true);
	mp.put("Combined_License_Application",filelist);
	info mp;
	update = zoho.crm.updateRecord("New_license_Forms",application_details.getjson("id").toLong(),mp,{"trigger":{"workflow"}});
	info "Update Application Download:" + update;
	//Update the License App Trigger to False
	app_map = Map();
	app_map.put("License_App_Trigger",false);
	app_res = zoho.crm.updateRecord("Applications1",application_id,app_map);
}
